gp0 = []
gp1 = []
gp2 = []
gp3 = []
gp4 = []
gp5 = []
gp6 = []
gp7 = []
gp8 = []
gp9 = []
x = [gp0,gp1,gp2,gp3,gp4,gp5,gp6,gp7,gp8,gp9]
n = input("Integer inputs: ").split()
for i in n:
  if i.isnumeric():
    if int(i) > 0:
      if i[-1] == '0':
        gp0.append(int(i))
      elif i[-1] == '1':
        gp1.append(int(i))
      elif i[-1] == '2':
        gp2.append(int(i))
      elif i[-1] == '3':
        gp3.append(int(i))
      elif i[-1] == '4':
        gp4.append(int(i))
      elif i[-1] == '5':
        gp5.append(int(i))
      elif i[-1] == '6':
        gp6.append(int(i))
      elif i[-1] == "7":
        gp7.append(int(i))
      elif i[-1] == "8":
        gp8.append(int(i))
      elif i[-1] == "9":
        gp9.append(int(i))


for i,j in enumerate(x):
  if j != []:
    a = sorted(j)
    print(f"Group {i}, Total {len(j)}, Values: {a}")
    
    
